def largest_of_two(a, b):

    if a == b:
        return 'Invalid'
    return max(a, b)

if __name__ == "__main__":
    import doctest
    doctest.testmod()

    # Manual tests
    print(largest_of_two(5, 10))     # 10
    print(largest_of_two(-3, -7))    # -3
    print(largest_of_two(0, 0))      # 'Invalid'
